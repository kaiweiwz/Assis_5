	private List<FeedbackMsg> feedbackMsgList;

	public FeedbackAdapter() {

	}

	public FeedbackAdapter(Activity mActivity, List<FeedbackMsg> list) {
		this.mActivity = mActivity;
		this.leftMsg = mActivity.getResources().getString(
				R.string.feedback_left_msg_1);
		this.feedbackMsgList = list;
	}

	@Override
	public int getCount() {
		feedbackMsgList.size();
		return 0;
	}

	@Override
